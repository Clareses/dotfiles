include ./theme.conf

# font_family      Jetbrains Mono Light
# bold_font        Jetbrains Mono Light Italic
# italic_font      Jetbrains Mono Light Italic
# bold_italic_font Jetbrains Mono Light Italic

# font_family      monego
# bold_font        Jetbrains Mono Light Italic
# italic_font      Jetbrains Mono Light Italic
# bold_italic_font Jetbrains Mono Light Italic


# font_family      Monaspace Radon Var
# bold_font        Monaspace Radon Var
# italic_font      Monaspace Radon Var
# bold_italic_font Monaspace Radon Var

# font_family      UbuntuMono Nerd Font Mono
# bold_font        UbuntuMono Nerd Font Mono
# italic_font      UbuntuMono Nerd Font Mono
# bold_italic_font UbuntuMono Nerd Font Mono

symbol_map U+4E00-U+9FFF,U+3400-U+4DBF YouYuan

font_size 12.3
font_weight 100

background_opacity 0.97

window_margin_width 0 4

confirm_os_window_close 2

cursor_trail 3


# BEGIN_KITTY_FONTS
font_family      family="Monego"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
